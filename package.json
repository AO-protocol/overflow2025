    "walrus:dev": "pnpm --filter walrus run dev",
    "walrus:verify": "pnpm --filter walrus run verify",
    "walrus:verify:prod": "pnpm --filter walrus run verify production",
    "walrus:verify:dev": "pnpm --filter walrus run verify development",
    "postinstall": "pnpm lefthook"
